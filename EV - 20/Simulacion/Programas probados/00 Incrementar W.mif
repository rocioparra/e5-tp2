-- Quartus Prime generated Memory Initialization File (.mif)

WIDTH=14;
DEPTH=2048;

ADDRESS_RADIX=HEX;
DATA_RADIX=BIN;

CONTENT BEGIN
	000  :   00010011111111;       -- MOV W,#0xFF
	001  :   00011100000001;       -- ADK W,0x01
	002  :   00011100000001;       -- ADK W,0x01
--	002  :   01011000000000;       -- MOM W,0x200
	003  :   00011100000100;       -- ADK W,0x01
	004  :   00011100000100;       -- ADK W,0x01
	[005..7FF]  :   00011100000100; -- ADK W,0x01
END;
